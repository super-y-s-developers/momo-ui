{
  "name": "@superys/momo-ui",
  "version": "0.0.14",
  "description": "A design system that works as a comic relief. It's light and fun",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/super-y-s-developers/momo-ui.git"
  },
  "contributors": [
    "Yeliana Torres <yelis124@gmail.com> (https://github.com/pollencio)",
    "Jesus F. Chavarro <jesusfchavarro@gmail.com> (https://github.com/jesusfchavarro)",
    "Kevin Malaver (https://github.com/kevinmalaverr)"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "build": "yarn build:esm || yarn build:cjs || true",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook -s public --docs",
    "chromatic": "npx chromatic --project-token 918e124b26dd"
  },
  "dependencies": {
    "polished": "^4.1.3",
    "styled-components": "^5.3.3",
    "styled-components-modifiers": "^1.2.5",
    "styled-system": "^5.1.5",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@storybook/theming": "^6.3.8",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.1.10",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/styled-components": "^5.1.15",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-dom": "^17.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/super-y-s-developers/momo-ui/issues"
  },
  "homepage": "https://github.com/super-y-s-developers/momo-ui#readme",
  "keywords": [
    "design",
    "system"
  ],
  "files": [
    "/lib"
  ]
}
